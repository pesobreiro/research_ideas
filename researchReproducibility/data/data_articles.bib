  journal = {Journal of strength and conditioning research},
  bibtexkey = {Flatt20201649},
  author = {Flatt, A.A., Esco, M.R., Allen, J.R., Robinson, J.B., Bragg, A., Keith, C.M., Fedewa, M.V., Earley, R.L.},
